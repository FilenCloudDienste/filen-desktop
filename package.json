{
	"name": "@filen/desktop",
	"version": "0.1.29",
	"buildNumber": 2023,
	"description": "Filen Desktop Client",
	"author": "Filen Cloud Dienste UG (haftungsbeschränkt) <support@filen.io>",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"private": false,
	"scripts": {
		"test": "jest --forceExit ./__tests__",
		"lint": "eslint src/**/* --ext .js,.jsx,.ts,.tsx",
		"emitTypes": "tsc --emitDeclarationOnly",
		"tsc": "tsc --build",
		"clear": "rimraf ./dist && rimraf ./prod",
		"build": "npm run clear && npm run lint && npm run tsc",
		"dev": "tsc ./src/preload.ts --outDir ./dist && cross-env NODE_ENV=development electron ./src/index.dev.js",
		"dev:virtualDrive": "cross-env NODE_ENV=development node ./src/virtualDrive/worker.dev.js",
		"dev:webdav": "cross-env NODE_ENV=development node ./src/webdav/worker.dev.js",
		"dev:sync": "cross-env NODE_ENV=development node ./src/sync/worker.dev.js",
		"dev:s3": "cross-env NODE_ENV=development node ./src/s3/worker.dev.js",
		"dev:dev": "tsx ./dev/index.ts",
		"dev:yalc": "npm run yalc && cross-env NODE_ENV=development electron ./src/index.dev.js",
		"yalc": "npm run build && yalc push",
		"build:mac": "npm run build && electron-builder -m --publish never",
		"build:win": "npm run build && electron-builder -w --publish never",
		"build:linux": "npm run build && electron-builder -l --publish never"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FilenCloudDienste/filen-desktop.git"
	},
	"keywords": [
		"filen"
	],
	"engines": {
		"node": ">=18"
	},
	"license": "AGPLv3",
	"bugs": {
		"url": "https://github.com/FilenCloudDienste/filen-desktop/issues"
	},
	"homepage": "https://filen.io",
	"devDependencies": {
		"@electron/notarize": "^2.3.2",
		"@jest/globals": "^29.7.0",
		"@types/fs-extra": "^11.0.4",
		"@types/lodash": "^4.14.202",
		"@types/mime-types": "^2.1.4",
		"@types/uuid": "^9.0.8",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"cross-env": "^7.0.3",
		"dotenv": "^16.4.5",
		"electron": "^31.3.0",
		"electron-builder": "^24.13.3",
		"eslint": "^8.56.0",
		"jest": "^29.7.0",
		"rimraf": "^5.0.5",
		"ts-node": "^10.9.2",
		"tsx": "^4.11.0",
		"typescript": "^5.3.3",
		"wait-on": "^7.2.0"
	},
	"dependencies": {
		"@filen/s3": "^0.2.13",
		"@filen/sdk": "^0.1.134",
		"@filen/sync": "^0.1.17",
		"@filen/web": "^0.1.12",
		"@filen/webdav": "^0.2.20",
		"axios": "^1.7.2",
		"diskusage-ng": "^1.0.4",
		"electron-updater": "^6.2.1",
		"find-free-ports": "^3.1.1",
		"fs-extra": "^11.2.0",
		"lodash": "^4.17.21",
		"mime-types": "^2.1.35",
		"tree-kill": "^1.2.2",
		"typed-emitter": "^2.1.0",
		"uuid": "^9.0.1"
	},
	"build": {
		"appId": "io.filen.desktop",
		"productName": "Filen",
		"asar": true,
		"npmRebuild": false,
		"nodeGypRebuild": false,
		"artifactName": "filen_${arch}.${ext}",
		"afterSign": "./build/notarize.js",
		"directories": {
			"output": "./prod/",
			"buildResources": "./build/"
		},
		"files": [
			"./node_modules/**/*",
			"./package.json",
			"./assets/**/*",
			"./dist/**/*",
			"./bin/**/*"
		],
		"publish": [
			{
				"provider": "generic",
				"url": "https://cdn.filen.io/desktop/release/"
			}
		],
		"win": {
			"publisherName": [
				"Filen Cloud Dienste UG (haftungsbeschränkt)"
			],
			"certificateSubjectName": "Filen Cloud Dienste UG (haftungsbeschränkt)",
			"verifyUpdateCodeSignature": false,
			"icon": "./build/icons/win/icon.ico",
			"target": {
				"target": "nsis",
				"arch": [
					"x64",
					"arm64"
				]
			},
			"publish": [
				{
					"provider": "generic",
					"url": "https://cdn.filen.io/desktop/release/"
				}
			],
			"sign": "./build/sign.js"
		},
		"nsis": {
			"oneClick": true,
			"createDesktopShortcut": true,
			"createStartMenuShortcut": true,
			"deleteAppDataOnUninstall": true,
			"allowToChangeInstallationDirectory": false,
			"allowElevation": true
		},
		"linux": {
			"maintainer": "Filen Cloud Dienste UG (haftungsbeschränkt)",
			"vendor": "Filen Cloud Dienste UG (haftungsbeschränkt)",
			"category": "Utility",
			"icon": "./assets/icons/png/",
			"target": {
				"target": "AppImage",
				"arch": [
					"x64",
					"arm64"
				]
			},
			"publish": [
				{
					"provider": "generic",
					"url": "https://cdn.filen.io/desktop/release/"
				}
			]
		},
		"mac": {
			"hardenedRuntime": true,
			"gatekeeperAssess": false,
			"entitlements": "./build/entitlements.mac.plist",
			"entitlementsInherit": "./build/entitlements.mac.plist",
			"category": "public.app-category.productivity",
			"darkModeSupport": true,
			"target": {
				"target": "default",
				"arch": [
					"x64",
					"arm64"
				]
			},
			"publish": [
				{
					"provider": "generic",
					"url": "https://cdn.filen.io/desktop/release/"
				}
			],
			"icon": "./build/icons/mac/icon.icns"
		},
		"dmg": {
			"sign": false
		}
	}
}
